@page
@model ProjektNYYYZealand.Pages.UnderviserPage.UpdateUnderviserModel
@{
    ViewData["Title"] = "Create Bog ";
}

<h1>Create</h1>
<div class="row">
    <div class="col-md-4">
        <form method="post">
            <div asp-validation-summary="  ModelOnly  " class="text-danger">
            </div>
            <div class="form-group">
                <label asp-for="@Model.underviser.UnderviserId" class="control-label"></label>
                <input asp-for="@Model.underviser.UnderviserId" class="form-control" />
                <span asp-validation-for="@Model.underviser.UnderviserId" class="text-danger"></span>
            </div>

            <div class="form-group">
                <label asp-for="@Model.underviser.MedarbejderNumber" class="control-label"></label>
                <input asp-for="@Model.underviser.MedarbejderNumber" class="form-control" />
                <span asp-validation-for="@Model.underviser.MedarbejderNumber" class="text-danger"></span>
            </div>

            <div class="form-group">
                <label asp-for="@Model.underviser.Name" class="control-label"></label>
                <input asp-for="@Model.underviser.Name" class="form-control" />
                <span asp-validation-for="@Model.underviser.Name" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="@Model.underviser.Email" class="control-label"></label>
                <input asp-for="@Model.underviser.Email" class="form-control" />
                <span asp-validation-for="@Model.underviser.Email" class="text-danger"></span>
            </div>
            <div class="form-group">
                <input type="submit" value="Update" class="btn btn-primary" />
            </div>
        </form>
    </div>
    <a asp-page="GetUnderviser">Tilbage</a>
</div>