@page
@model ProjektNYYYZealand.Pages.HoldPage.DeleteHoldModel
@{
    ViewData["Title"] = "Delete Hold ";
}

<h1>Delete Hold</h1>
<h3>Are you sure you want to delete this?</h3>
<div>
    <form method="post">
        <dl class="row">
            <dd class="col-sm-10">
                @Html.DisplayFor(model => model.hold.HoldId)
            </dd>
            <dt class="col-sm-2">
                @Html.DisplayNameFor(model => model.hold.Holdnavn)
            </dt>
            <dd class="col-sm-10">
                @Html.DisplayFor(model => model.hold.Semester)
            </dd>
            <dt class="col-sm-2">
                @Html.DisplayNameFor(model => model.hold.Uddannelse)
            </dt>
        </dl>
        <input type="submit" value="Delete" class="btn btn-danger" />
    </form>
    <div>
        <a asp-page="GetHold">Tilbage</a>
    </div>
</div>