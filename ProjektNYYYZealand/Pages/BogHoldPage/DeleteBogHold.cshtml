@page
@model ProjektNYYYZealand.Pages.BogHoldPage.DeleteBogHoldModel
@{
    ViewData["Title"] = "Delete BogHold ";
}

<h1>Delete UnderviserHold</h1>
<h3>Are you sure you want to delete this?</h3>
<div>
    <form method="post">
        <dl class="row">
            <dt class="col-sm-2">
                @Html.DisplayNameFor(model => model.bogHold.BogHoldId)
            </dt>
            <dd class="col-sm-10">
                @Html.DisplayFor(model => model.bogHold.BogId)
            </dd>
            <dd class="col-sm-10">
                @Html.DisplayFor(model => model.bogHold.HoldId)
            </dd>

        </dl>
        <input type="submit" value="Delete" class="btn btn-danger" />
    </form>
    <div>
        <a asp-page="GetBogHoldPage">Tilbage</a>
    </div>
</div>
