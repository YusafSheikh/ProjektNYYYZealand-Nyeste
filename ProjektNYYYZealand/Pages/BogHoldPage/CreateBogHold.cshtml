@page
@model ProjektNYYYZealand.Pages.BogHoldPage.CreateBogHoldModel
@{
    ViewData["Title"] = "Create BogHold ";
}

<h1>Create</h1>
<div class="row">
    <div class="col-md-4">
        <form method="post">
            <div asp-validation-summary="  ModelOnly  " class="text-dark">
            </div>
            <div class="form-group">
                <label asp-for="@Model.bogHold.BogHoldId" class="control-label"></label>
                <input asp-for="@Model.bogHold.BogHoldId" class="form-control" />
                <span asp-validation-for="@Model.bogHold.BogHoldId" class="text-danger"></span>
            </div>

            <div class="form-group">
                <label asp-for="@Model.bogHold.BogId" class="control-label"></label>
                <input asp-for="@Model.bogHold.BogId" class="form-control" />
                <span asp-validation-for="@Model.bogHold.BogId" class="text-danger"></span>
            </div>

            <div class="form-group">
                <label asp-for="@Model.bogHold.HoldId" class="control-label"></label>
                <input asp-for="@Model.bogHold.HoldId" class="form-control" />
                <span asp-validation-for="@Model.bogHold.HoldId" class="text-danger"></span>
            </div>


            <div class="form-group">
                <input type="submit" value="Create" class="btn btn-light btn-outline-dark" />
            </div>
        </form>
    </div>
    <a asp-page="BogHoldPage">Tilbage</a>
</div>
